<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>

<ScrollPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dschapplication.DSCHController">
	<content>
		<AnchorPane prefHeight="798.0" prefWidth="798.0">
         <children>
            <TabPane prefHeight="798.0" prefWidth="798.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab text="User">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <TitledPane animated="false" layoutX="14.0" layoutY="14.0" prefHeight="126.0" prefWidth="774.0" text="BlockChain Network Connection">
                                <content>
                                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                       <children>
                                          <Label layoutX="14.0" layoutY="14.0" text="ConnectionID   :" />
                                          <Label layoutX="14.0" layoutY="50.0" text="ConnectionOrg :" />
                                          <TextField fx:id="userIDfield" layoutX="130.0" layoutY="9.0" prefHeight="26.0" prefWidth="233.0" />
                                          <TextField fx:id="userOrgfield" layoutX="130.0" layoutY="45.0" prefHeight="26.0" prefWidth="234.0" />
                                          <Label layoutX="14.0" layoutY="77.0" text="Connection status:" />
                                          <Label layoutX="368.0" layoutY="50.0" text="(Org1, Org2, Org3, ...)" />
                                          <Button layoutX="594.0" layoutY="45.0" mnemonicParsing="false" onAction="#loginUser" text="Login" />
                                          <Button layoutX="519.0" layoutY="45.0" mnemonicParsing="false" onAction="#registerUser" text="Resister" />
                                       </children>
                                    </AnchorPane>
                                </content>
                              </TitledPane>
                              <TitledPane animated="false" layoutX="14.0" layoutY="162.0" prefHeight="593.0" prefWidth="775.0" text="Distribution Participants status">
                                <content>
                                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                       <children>
                                          <Label layoutX="72.0" layoutY="22.0" text="User List" />
                                          <TableView fx:id="userListTableView" layoutX="5.0" layoutY="42.0" prefHeight="470.0" prefWidth="190.0">
                                            <columns>
                                              <TableColumn fx:id="userIDColumn" prefWidth="189.0" text="User ID" />
                                            </columns>
                                          </TableView>
                                          <TableView fx:id="userAssetStatusTableView" layoutX="411.0" layoutY="42.0" prefHeight="471.0" prefWidth="324.0">
                                            <columns>
                                              <TableColumn fx:id="userInfoNameColumn" prefWidth="162.0" text="Name" />
                                              <TableColumn fx:id="userInfoValueColumn" prefWidth="161.0" text="Value" />
                                            </columns>
                                          </TableView>
                                          <Label layoutX="516.0" layoutY="22.0" text="User Asset Status" />
                                          <Label layoutX="5.0" layoutY="529.0" text="Asset Name: " />
                                          <Label layoutX="282.0" layoutY="529.0" text="Asset Quantity:" />
                                          <TextField fx:id="assetNamefield" layoutX="92.0" layoutY="524.0" />
                                          <TextField fx:id="assetQuantityfield" layoutX="385.0" layoutY="524.0" />
                                          <Button layoutX="603.0" layoutY="524.0" mnemonicParsing="false" onAction="#buyingAsset" text="Buying" />
                                          <Button layoutX="673.0" layoutY="524.0" mnemonicParsing="false" onAction="#sellingAsset" text="Selling" />
                                       </children>
                                    </AnchorPane>
                                </content>
                              </TitledPane>
                           </children>
                        </AnchorPane>
                  </content>
                </Tab>
                <Tab text="WholeSale">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                  </content>
                </Tab>
                  <Tab text="LocalSale">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                  </Tab>
                  <Tab text="Distribution">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                  </Tab>
              </tabs>
            </TabPane>
         </children>
       </AnchorPane>
   </content>
</ScrollPane>
